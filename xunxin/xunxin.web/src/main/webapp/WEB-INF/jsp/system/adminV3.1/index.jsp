				if(data.indexOf('###')!=-1){
					//消息类型
					msgType = data.split('###')[0];
					//消息内容
					msgTxt = decodeURI(data.split('###')[1]);
					//消息序号
					crtNo = data.split('###')[2];
					doToast(msgType,msgTxt,'提醒',null);
				}
				if(msgType!='syserror'){
					//递归调用，获取消息
					getNewMsg(crtNo);
				}
			},error: function(XMLHttpRequest, textStatus, errorThrown) {
                alert('请检查是否网络异常');
                location.reload();
                
            }
		});
    }
    //获取当前用户最新消息，默认从第0条开始
    getNewMsg(0);
    </script>
</body>

</html>
