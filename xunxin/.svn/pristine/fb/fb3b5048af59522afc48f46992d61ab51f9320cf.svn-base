public class QuestionOperationRecord extends GeneralBean{

    /**
     * 
     */
    private static final long serialVersionUID = 1L;
    
    private int                              adminId;               //操作人
    private int                              OperationContent;      //操作事项
    private int                              OperationState;        //执行状态
    private int                              OperationRemark;       //备注    
    
    public int getAdminId() {
        return adminId;
    }
    public void setAdminId(int adminId) {
        this.adminId = adminId;
    }
    public int getOperationContent() {
        return OperationContent;
    }
    public void setOperationContent(int operationContent) {
        OperationContent = operationContent;
    }
    public int getOperationState() {
        return OperationState;
    }
    public void setOperationState(int operationState) {
        OperationState = operationState;
    }
    public int getOperationRemark() {
        return OperationRemark;
    }
    public void setOperationRemark(int operationRemark) {
        OperationRemark = operationRemark;
    }
    
    
    
    
    
    
    
}
