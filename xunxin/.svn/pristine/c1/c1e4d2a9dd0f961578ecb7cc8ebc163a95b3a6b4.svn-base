    <select id="findMatchsByUserId" resultMap="BaseResultFriendMap" parameterType="java.lang.Integer" >
		    SELECT
				s.id,
				s.user_id,
				s.consonance_id,
				s.is_delete,
				s.createDate,
				s.updateDate,
				s.match_value,
			 	u.nickName,
			 	u.gender,
			 	u.is_login,
			 	s.is_read
			 	
			FROM
				xunxin_user_match s
			LEFT JOIN xunxin_user u ON s.consonance_id = u.uid
			<where>
				s.user_id = #{userId,jdbcType=INTEGER}
				and s.is_delete = 0
			</where>
			ORDER BY s.updateDate desc	

  </select>
