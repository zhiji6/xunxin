public class UserSelfPortraitVO {

	private int 								id;
	private String								usualInfo;		 //固定信息			
	private String 								extraInfo;		 //拓展信息
	private Date								updateTime;		 //修改时间	
	private int                                 userId;			 //用户
	
	public UserSelfPortraitVO() {
        super();
    }
    public UserSelfPortraitVO(String usualInfo, String extraInfo, Date updateTime, int userId) {
        super();
        this.usualInfo = usualInfo;
        this.extraInfo = extraInfo;
        this.updateTime = updateTime;
        this.userId = userId;
    }
    public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getUsualInfo() {
		return usualInfo;
	}
	public void setUsualInfo(String usualInfo) {
		this.usualInfo = usualInfo;
	}
	public String getExtraInfo() {
		return extraInfo;
	}
	public void setExtraInfo(String extraInfo) {
		this.extraInfo = extraInfo;
	}
	public Date getUpdateTime() {
		return updateTime;
	}
	public void setUpdateTime(Date updateTime) {
		this.updateTime = updateTime;
	}
	public int getUserId() {
		return userId;
	}
	public void setUserId(int userId) {
		this.userId = userId;
	}
	
	
	
	
}
