package com.xunxin.util.app.jsonInterceptor;

import java.util.Map;

import org.springframework.http.MediaType;

public class MYMediaType extends MediaType{

	public MYMediaType(MediaType other, Map<String, String> parameters) {
		super(other, parameters);
	}
	public static final MediaType APPLICATION_JSON_UTF8 = valueOf("application/json;charset=UTF-8");
	
	public static final String APPLICATION_JSON_UTF8_VALUE = "application/json;charset=UTF-8";

}
